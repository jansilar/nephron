\begin_layout Standard
We follow the derivation from Hoppenstadt (4.2.1) - (4.3.22).
 We refuse the assumption of chapter 4.4.
 We accept assumptions of chapter 4.5.
 
\end_layout

\begin_layout Standard
ADH enables water recovery.
\end_layout

\begin_layout Subsubsection*
Distal tubulus
\end_layout

\begin_layout Standard
Concentrations in distal tubulus entry
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
c_{DT}(0)=c_{0}e^{\alpha}(1-\alpha)\label{eq:cDT0}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Without ADH, 
\begin_inset Formula $c_{DT}$
\end_inset

 is constant in the DT.
 With full ADH, it equilibrate with 
\begin_inset Formula $c(0)$
\end_inset

 in the end of DT:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
c_{DT}^{ADH}(L_{DT})=c_{0}\label{eq:cDTADHL}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
With arbitrary ADH
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
c_{DT}(L_{DT}) & =(1-ADH)\cdot c_{DT}(0)+ADH\cdot c_{DT}^{ADH}(L_{DT})\\
 & =(1-ADH)\cdot c_{0}e^{\alpha}(1-\alpha)+ADH\cdot c_{0}\\
 & =c_{0}\cdot\left[(1-ADH)\cdot e^{\alpha}(1-\alpha)+ADH\right]
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Along the distal tubulus the 
\begin_inset Formula $c_{DT}$
\end_inset

 linearly interpolates from (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:cDT0"

\end_inset

) to (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:cDTADHL"

\end_inset

)
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
c_{DT}^{ADH}(x)=c_{0}e^{\alpha}(1-\alpha)+\frac{x}{L_{DT}}\cdot c_{0}\cdot\left[1-e^{\alpha}(1-\alpha)\right]
\]

\end_inset


\end_layout

\begin_layout Standard
With partial ADH 
\begin_inset Formula $ADH\in(0,1)$
\end_inset

 the letter term is multiplied by ADH
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
c_{DT}(x) & =c_{0}e^{\alpha}(1-\alpha)+ADH\cdot\frac{x}{L_{DT}}\left[c_{0}-c_{0}e^{\alpha}(1-\alpha)\right]\\
 & =c_{0}\left(e^{\alpha}(1-\alpha)+ADH\cdot\frac{x}{L_{DT}}\left[1-e^{\alpha}(1-\alpha)\right]\right)\\
 & =c_{0}\left[\left(1-ADH\cdot\frac{x}{L_{DT}}\right)\cdot e^{\alpha}(1-\alpha)+ADH\cdot\frac{x}{L_{DT}}\right]
\end{align*}

\end_inset


\end_layout

\begin_layout Subsubsection*
Collecting duct
\end_layout

\begin_layout Standard
inlet
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
c_{CD}(0)=c_{DT}(L_{DT})=c_{0}\left[\left(1-ADH\right)\cdot e^{\alpha}(1-\alpha)+ADH\right]
\]

\end_inset


\end_layout

\begin_layout Standard
outlet
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
c_{CD}(L) & =(1-ADH)\cdot c_{DT}(0)+ADH\cdot c(L)\\
 & =(1-ADH)\cdot c_{0}e^{\alpha}(1-\alpha)+ADH\cdot c_{0}e^{\alpha}\\
 & =c_{0}\left[(1-ADH)\cdot e^{\alpha}(1-\alpha)+ADH\cdot e^{\alpha}\right]
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
inlet outlet difference
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\Delta c_{CD} & =c_{CD}(L)-c_{CD}(0)\\
 & =c_{0}\left[(1-ADH)\cdot e^{\alpha}(1-\alpha)+ADH\cdot e^{\alpha}\right]-c_{0}\left[\left(1-ADH\right)\cdot e^{\alpha}(1-\alpha)+ADH\right]\\
 & =c_{0}\left((1-ADH)\cdot e^{\alpha}(1-\alpha)+ADH\cdot e^{\alpha}-\left(1-ADH\right)\cdot e^{\alpha}(1-\alpha)-ADH\right)\\
 & =c_{0}\left(ADH\cdot e^{\alpha}-ADH\right)\\
 & =c_{0}ADH\left(e^{\alpha}-1\right)
\end{align*}

\end_inset

and generally
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
c_{CD}(x) & =c_{CD}(0)+\frac{x}{L}\Delta c_{CD}\\
 & =c_{0}\left[\left(1-ADH\right)\cdot e^{\alpha}(1-\alpha)+ADH\right]+\frac{x}{L}c_{0}ADH\left(e^{\alpha}-1\right)\\
 & =c_{0}\left[\left(1-ADH\right)\cdot e^{\alpha}(1-\alpha)+ADH+\frac{x}{L}ADH\left(e^{\alpha}-1\right)\right]
\end{align*}

\end_inset


\end_layout

